from collections import deque

def minMutation(start, end, bank):
    bank = set(bank)

    if end not in bank:
        return -1

    q = deque([(start, 0)])
    while q:
        gene, steps = q.popleft()
        if gene == end:
            return steps
        for i in range(len(gene)):
            for ch in "ACGT":
                mutated = gene[:i] + ch + gene[i+1:]
                if mutated in bank:
                    bank.remove(mutated)
                    q.append((mutated, steps + 1))

    return -1


# Example
print(minMutation("AACCGGTT", "AAACGGTA", ["AACCGGTA", "AACCGCTA", "AAACGGTA"]))
